{
  "name": "express-msal-auth",
  "version": "1.0.1",
  "description": "A Node.js & Express web app calling Microsoft Graph API using MSAL Node",
  "main": "src/app.js",
  "scripts": {
    "start": "STORAGEACCOUNTKEY=$(az storage account keys list -n $prefix --query [0].value -o tsv) STORAGEACCOUNTNAME=$prefix node src/app.js",
    "dev": "nodemon src/app.js ./.env.dev"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/core-http": "^3.0.4",
    "@azure/identity": "^1.5.2",
    "@azure/ms-rest-nodeauth": "^3.1.0",
    "@azure/msal-common": "^4.2.1",
    "@azure/msal-node": "^1.0.3",
    "@azure/storage-blob": "^12.8.0",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.3",
    "uuid": "^8.3.1",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      "src/data/cache.json",
      "README.md"
    ]
  }
}
